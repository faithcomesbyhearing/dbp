{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/hotmeteor/Generators.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "algolia/algoliasearch-client-php": "^3.0",
        "appstract/laravel-opcache": "^4.0",
        "aws/aws-sdk-php-laravel": "^3.6",
        "bugsnag/bugsnag-laravel": "^2.0",
        "curl/curl": "^1.9",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.11",
        "google/recaptcha": "^1.1",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.4",
        "josiasmontag/laravel-localization": "^0.8.2",
        "laravel/framework": "^8.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.6",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maennchen/zipstream-php": "^0.5.1",
        "mattkingshott/quest": "^1.1",
        "sentry/sentry-laravel": "^2.8",
        "socialiteproviders/generators": "dev-php-8",
        "socialiteproviders/google": "^4.1",
        "spatie/array-to-xml": "^2.7",
        "spatie/eloquent-sortable": "^3.6",
        "spatie/laravel-fractal": "^5.8",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "symfony/process": "^5.3",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/yaml": "^5.3",
        "torann/geoip": "^3.0",
        "yosymfony/toml": "^1.0",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^5.1",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^5.8",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-tail": "^4.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
		"files": [
			"app/Helpers/Helpers.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
	  	"cs": [
		  	"php-cs-fixer fix --dry-run"
	  	],
	  	"cs-fix": [
		  	"php-cs-fixer fix"
	  	]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
